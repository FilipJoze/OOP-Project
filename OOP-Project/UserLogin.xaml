<Window x:Class="OOP_Project.UserLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOP_Project"
        mc:Ignorable="d"
        Title="UserLogin" Height="450" Width="350">
    <Grid>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Height="54" Margin="30,205,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="251" FontSize="36"/>
        <TextBox x:Name="txtUsername_Copy" HorizontalAlignment="Left" Height="54" Margin="30,311,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="251" FontSize="36"/>
        <Label x:Name="lblUsername" Content="Username" HorizontalAlignment="Left" Margin="30,158,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label x:Name="lblPassword" Content="Password" HorizontalAlignment="Left" Margin="30,264,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label x:Name="lblCreateAccount" Content="Dont have an account?" HorizontalAlignment="Left" Margin="86,370,0,0" VerticalAlignment="Top"/>
        <Image Margin="74,0,89,252" Source="Image1.png" Stretch="Fill"/>
    </Grid>
</Window>
